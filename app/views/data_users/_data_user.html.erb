<% data_users.each do |data| %>
    <%= image_tag data.picture.thumb.url, class: 'figure-img img-fluid rounded' %>
    <br>
    <%= data.nick %>
    <br>
    <%= data.information %>
    <br>
    <%= link_to 'Edit', edit_data_user_path(data) %>
    <%= link_to 'Destroy', data, method: :delete, data: { confirm: 'sure?'} %> <br>
<% end %>